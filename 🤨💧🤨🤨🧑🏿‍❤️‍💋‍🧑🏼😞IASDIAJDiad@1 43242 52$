TrollSection:NewButton("Hat Resize R15", "Makes ur hats massive only works with rthro hats", function()
    for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v.Name == "OriginalSize" then
            v:Destroy()
        end
    end
    game.Players.LocalPlayer.Character.Humanoid:FindFirstChild("BodyDepthScale"):Destroy()
    wait(1)
    for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v.Name == "OriginalSize" then
            v:Destroy()
        end
    end
    game.Players.LocalPlayer.Character.Humanoid:FindFirstChild("BodyTypeScale"):Destroy()
    wait(1)
    for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v.Name == "OriginalSize" then
            v:Destroy()
        end
    end
    game.Players.LocalPlayer.Character.Humanoid:FindFirstChild("HeadScale"):Destroy()
    for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v.Name == "OriginalSize" then
            v:Destroy()
        end
    end
    game.Players.LocalPlayer.Character.Humanoid:FindFirstChild("BodyHeightScale"):Destroy()
    wait(1)
    for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v.Name == "OriginalSize" then
            v:Destroy()
        end
    end
    game.Players.LocalPlayer.Character.Humanoid:FindFirstChild("BodyWidthScale"):Destroy()
    wait(1)
    for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v.Name == "OriginalSize" then
            v:Destroy()
        end
    end
    game.Players.LocalPlayer.Character.Humanoid:FindFirstChild("BodyProportionScale"):Destroy()
    for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if v.Name == "OriginalSize" then
            v:Destroy()
        end
    end
end)










MainSection:NewButton("Gun mods (buggy)", "Works in games with original roblox guns", function()
	for i,v in next, getgc(true) do
		if type(v) == "table" and v.FireRate then
			v.AmmoPerClip = math.huge
			v.Spread = 0
			v.FireRate = 0
			v.Auto = true
			v.ReloadTime = 0
			v.MaxClip = math.huge
			v.BaseDamage = math.huge
			v.FlamingBullet = true
			v.MouseSensitive = 0
			v.Intensity = math.huge
			v.CameraShakingEnabled = false
			v.Piercing = math.huge
			v.Lifesteal = math.huge
			v.ChargingTime = 0
			v.Clips = math.huge
			v.LimitedClipEnabled = false
			v.HeadshotEnabled = true
			v.HeadshotDamageMultiplier = math.huge
			v.ReloadAnimationSpeed = 0
			v.Knockback = 9999999
			v.AmmoPerMag = math.huge
			v.Range = math.huge
			v.MaxAmmo = math.huge
			v.Ammo = math.huge
			v.Recoil = 0
			v.AngleX = 0
			v.AngleY = 0
			v.RecoilSpeed = 0
			v.VisualizerEnabled = true
			v.MagSize = math.huge
			v.AutoFire = true
			v.ShotgunReload = false
			v.CurrentAmmo = math.huge
			v.StoredAmmo = math.huge
			v.Damage = math.huge
		end
	end
end)	



