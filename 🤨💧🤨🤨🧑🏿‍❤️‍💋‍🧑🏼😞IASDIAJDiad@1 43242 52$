CrimSection:NewToggle("Auto claim allowance", "made by Shafer#8116 🤤😍", function(state)
    if state then
        _G.AutoClaimAllowance = true


		while _G.AutoClaimAllowance do

			for _FORV3, _FORV4 in pairs(game:GetService("Workspace").Map.ATMz:GetChildren()) do
				game:GetService("ReplicatedStorage").Events.CLMZALOW:InvokeServer(_FORV4.MainPart)
			end
		end
    else
        _G.AutoClaimAllowance = false
    end
end)




